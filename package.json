{
  "name": "datocms-plugin-netlify-forms",
  "version": "0.3.3",
  "private": false,
  "type": "module",
  "description": "Plugin that allows DatoCMS show and manage Netlify form submissions",
  "homepage": "https://github.com/gempi/datocms-plugin-netlify-forms#readme",
  "author": "Matthias Gemperli <hello@matthiasgemperli.ch>",
  "license": "ISC",
  "keywords": [
    "datocms",
    "datocms-plugin",
    "netlify"
  ],
  "files": [
    "dist",
    "docs"
  ],
  "datoCmsPlugin": {
    "title": "Netlify Forms",
    "coverImage": "docs/cover.png",
    "previewImage": "docs/demo.mp4",
    "entryPoint": "dist/index.html",
    "permissions": []
  },
  "dependencies": {
    "datocms-plugin-sdk": "^2.0.9",
    "datocms-react-ui": "^2.0.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "netlify": "^13.1.17",
    "react-final-form": "^6.5.9"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "globals": "^15.9.0",
    "typescript": "^5.5.3",
    "vite": "^5.4.1"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
