{
  "name": "datocms-plugin-netlify-forms",
  "version": "0.2.7",
  "description": "Plugin that allows DatoCMS show and manage Netlify form submissions",
  "homepage": "https://github.com/gempi/datocms-plugin-netlify-forms#readme",
  "author": "Matthias Gemperli <hello@matthiasgemperli.ch>",
  "license": "ISC",
  "keywords": [
    "datocms",
    "datocms-plugin",
    "netlify"
  ],
  "files": [
    "docs",
    "build"
  ],
  "datoCmsPlugin": {
    "title": "Netlify Forms",
    "coverImage": "docs/cover.png",
    "previewImage": "docs/demo.mp4",
    "entryPoint": "build/index.html",
    "permissions": []
  },
  "dependencies": {
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "datocms-plugin-sdk": "^0.7.14",
    "datocms-react-ui": "^0.7.14",
    "netlify": "^13.1.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-final-form": "^6.5.9",
    "react-scripts": "^5.0.1",
    "typescript": "^5.1.6"
  },
  "scripts": {
    "start": "cross-env BROWSER='none' PUBLIC_URL='/' react-scripts start",
    "build": "cross-env PUBLIC_URL='.' react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepublishOnly": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gempi/datocms-plugin-netlify-forms.git"
  },
  "bugs": {
    "url": "https://github.com/gempi/datocms-plugin-netlify-forms/issues"
  }
}
